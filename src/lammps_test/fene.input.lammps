atom_style	bond

read_data	fene.lammps

pair_style	lj/cut 2.5
pair_coeff	1 1 1.0 1.0
pair_coeff	1 2 1.5 0.5
pair_coeff	2 2 0.8 0.88

bond_style	fene
bond_coeff	1 1.0 3.0 1.0 1.0

timestep	0.003

run_style 	verlet
fix		1 all nve

neigh_modify	every 1
thermo		1
thermo_style	custom step ke pe etotal
thermo_modify	norm no
dump		1 all xyz 1 output.lammps
dump_modify 	1 element A B

run		1000

